{
  "name": "buttplug-playground",
  "version": "0.0.1",
  "description": "Connectivity and Device Testing/Tutorial for Buttplug Websockets",
  "author": "Kyle Machulis <kyle@machul.is>",
  "private": true,
  "scripts": {
    "start": "yarn build:config && webpack-dev-server --hot --config build/webpack.base.js",
    "start:remote": "yarn build:config && webpack-dev-server --hot --host 192.168.123.148 --config build/webpack.base.js",
    "start:secure": "yarn build:config && webpack-dev-server --hot --https --config build/webpack.base.js",
    "start:remote:secure": "yarn build:config && webpack-dev-server --hot --https --host 192.168.123.148 --config build/webpack.base.js",
    "build:config": "node build/generate_config.js",
    "build:dev": "yarn build:config && webpack --progress --hide-modules --config build/webpack.base.js",
    "build": "yarn build:config && webpack --progress --hide-modules --config build/webpack.production.js",
    "tslint": "tslint -c tslint.json 'src/**/*.ts' 'tests/**/*.ts'",
    "tslint:fix": "tslint -c tslint.json --fix semicolon,comma,no-var-keyword,ordered-imports 'src/**/*.ts' 'tests/**/*.ts'",
    "webpack-defaults": "webpack-defaults",
    "build:main": "yarn build:config && tsc -p tsconfig.json",
    "pretest": "yarn build:main",
    "deploy": "trash deploy && mkdir -p deploy && cp index.html deploy && cp -r dist deploy"
  },
  "dependencies": {
    "buttplug": "^0.11.7",
    "matomo-tracker": "^2.2.1",
    "tslint": "^5.18.0",
    "viewport-units-buggyfill": "^0.6.2",
    "vue": "^2.6.10",
    "vue-awesome": "^3.5.4",
    "vue-buttplug-material-component": "^0.3.1",
    "vue-showdown": "^2.4.1",
    "vue-slider-component": "^2.8.16",
    "vue-touch": "next",
    "vuetify": "^1.5.16"
  },
  "devDependencies": {
    "@types/node": "^12.0.10",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "file-loader": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^1.3.7",
    "html-loader": "^0.5.5",
    "noop-loader": "^1.0.0",
    "null-loader": "^3.0.0",
    "raw-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "terser": "^4.0.2",
    "terser-webpack-plugin": "^1.3.0",
    "trash": "^6.0.0",
    "trash-cli": "^3.0.0",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2",
    "url-loader": "^2.0.1",
    "vue-loader": "^15.7.0",
    "vue-property-decorator": "^8.2.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "pre-push": []
}
